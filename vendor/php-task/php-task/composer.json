    "description": "Library to work with synchronous and asynchronous tasks in php.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Wachter",
            "email": "johannes.wachter@massiveart.at"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0",
        "doctrine/collections": "^1.0",
        "ramsey/uuid": "^3.1 || ^4.0",
        "symfony/event-dispatcher": "^2.6 || ^3.4 || ^4.0 || ^5.0",
        "dragonmantank/cron-expression": "^1.1 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.7",
        "psr/log": "^1.0",
        "symfony/phpunit-bridge": "^5.2.3"
    },
    "autoload": {
        "psr-4": {
            "": "src"
